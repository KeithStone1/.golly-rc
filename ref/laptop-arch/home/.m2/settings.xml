<settings>
	<mirrors>
		<mirror>
			<id>golly-central</id>
			<name>golly-svr1 Central proxy</name>
			<url>http://golly-svr1:8081/nexus/content/repositories/central/</url>
			<mirrorOf>central</mirrorOf>
		</mirror>
	</mirrors>

	<profiles>
		<profile>
			<id>nexus</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<nexus.url>http://golly-svr1:8081/nexus</nexus.url>
			</properties>
			<repositories>
				<repository>
					<id>golly-public</id>
					<url>http://golly-svr1:8081/nexus/content/groups/public/</url>
					<releases><enabled>true</enabled></releases>
					<snapshots><enabled>true</enabled></snapshots>
				</repository>
				<repository>
					<id>golly-private</id>
					<url>http://golly-svr1:8081/nexus/content/groups/private/</url>
					<releases><enabled>true</enabled></releases>
					<snapshots><enabled>true</enabled></snapshots>
				</repository>
			</repositories>
		</profile>
		<profile>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<sign.keystore>xxxxxxxx</sign.keystore>
				<sign.alias>xxxxxxxx</sign.alias>
				<sign.keypass>xxxxxxxx</sign.keypass>
				<sign.storepass>xxxxxxxx</sign.storepass>
			</properties>
		</profile>
	</profiles>

	<servers>
		<server>
			<id>sonatype-nexus-snapshots</id>
			<username>xxxxxxxx</username>
			<password>xxxxxxxx</password>
		</server>
		<server>
			<id>sonatype-nexus-staging</id>
			<username>xxxxxxxx</username>
			<password>xxxxxxxx</password>
		</server>
	</servers>
</settings>
